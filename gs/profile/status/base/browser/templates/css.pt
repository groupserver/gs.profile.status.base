<tal:block xmlns:tal="http://xml.zope.org/namespaces/tal">
.gs-profile-status-groups-site-odd {
    background-color: #fcfcfc;
    border-top: 1px #cccccc solid;
    border-bottom: 1px #cccccc solid;
}
.gs-profile-status-groups-site-even {
    background-color: #ebebeb;
}
.gs-profile-status-groups-site-odd:hover {
    background-color: #ffffff;
}
.gs-profile-status-groups-site-even:hover {
    background-color: #ffffff;
}
#gs-profile-status-groups:last-child {
    border-bottom: 1px #cccccc solid;
}
.gs-profile-status-groups-group-info-details td {
    padding: 0 0 0 20px;
}
.gs-profile-status-groups-group-info-details p {
    max-width: 580px; /* 600 - 20px [padding] */
}
#gs-profile-status-profile p {
    max-width: 520px; /* 600 - (70 [img] + 10 [padding]) */
}
#gs-profile-status-profile .well {
    max-width: 520px; /* 600 - (70 [img] + 10 [padding] */
                      /*        + (1 [border] + 9 [padding])*2) */
}
#gs-profile-status-profile-image-change, 
#gs-profile-status-profile-image-add {
    width: 56px; /* 70px [img width] - ((1px [border] + 6px [padding]) * 2) */
}
#gs-profile-status-profile-head {
    margin: 0 0 0 0;
    padding: 0 0 10px 10px;
}
#gs-profile-status-profile-h {
    margin: 0 0 0 0;
    padding: 0 0 10px 0;
    line-height: 20px;
}
p.well {
    margin: 0 0 0 0;
    margin-bottom: 0;
}
#gs-profile-status-profile-info td {
    padding: 0 0 10px 10px;
}
#gs-profile-status-profile-info-bio-content p {
    margin: 0 0 0 0;
    padding: 0 0 10px 0;
}
</tal:block>
